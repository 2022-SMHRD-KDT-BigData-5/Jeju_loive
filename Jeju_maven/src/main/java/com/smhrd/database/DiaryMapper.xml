<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.smhrd.domain.DiaryDAO">
	<insert id="insertdiary" parameterType="com.smhrd.domain.Member">
		insert into member values(#{id}, #{pw}, #{name}, #{tel})
	</insert>
	<select id="selectdiary" parameterType="com.smhrd.domain.Member" resultType="com.smhrd.domain.Member">
		select * from member where mem_id=#{id} and mem_pw=#{pw}
	</select>
	
</mapper>